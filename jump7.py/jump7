for i in range(1,101):
    if (i % 7 ==0 ) or (i % 10 == 7) or (i // 10 == 7) : 
        continue
    else:
        print(i) 


for i in range(1,101):
    if i % 7 == 0:
        continue
    elif i % 10 == 7:
        continue
    elif i // 10 == 7:
        continue
    else:
        print(i)


i = 1
while i <=100:
    if i % 7 ==0:
        pass
    elif i % 10 ==7:
        pass
    elif i // 10 ==7:
        pass
    else:
        print(i)
    i = i + 1
